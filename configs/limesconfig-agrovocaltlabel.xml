<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE LIMES SYSTEM "limes.dtd">
<LIMES>
<PREFIX>
  <NAMESPACE>http://www.w3.org/2002/07/owl#</NAMESPACE>
  <LABEL>owl</LABEL>
</PREFIX>
<PREFIX>
  <NAMESPACE>http://www.w3.org/2004/02/skos/core#</NAMESPACE>
  <LABEL>skos</LABEL>
</PREFIX>
<PREFIX>
  <NAMESPACE>http://schema.org/</NAMESPACE>
  <LABEL>url</LABEL>
</PREFIX>
<PREFIX>
  <NAMESPACE>http://www.w3.org/1999/02/22-rdf-syntax-ns#</NAMESPACE>
  <LABEL>rdf</LABEL>
</PREFIX>
<PREFIX>
  <NAMESPACE>http://dbpedia.org/ontology/</NAMESPACE>
  <LABEL>dbpo</LABEL>
</PREFIX>
<PREFIX>
  <NAMESPACE>http://www.w3.org/2000/01/rdf-schema#</NAMESPACE>
  <LABEL>rdfs</LABEL>
</PREFIX>
<SOURCE>
    <ID>data_source</ID>
    <ENDPOINT>/Users/gatemezing/Mondeca/projects/ProjetINSEE/limes/limes-core/concepts-local.ttl</ENDPOINT>
    <VAR>?l</VAR>
    <PAGESIZE>5000</PAGESIZE>
    <RESTRICTION>?l rdf:type skos:Concept</RESTRICTION>
    <PROPERTY>skos:prefLabel</PROPERTY>
    <PROPERTY>skos:altLabel</PROPERTY>
    <PROPERTY>skos:prefLabel AS lowercase RENAME inseename</PROPERTY>
    <PROPERTY>skos:altLabel AS lowercase RENAME inseealtname</PROPERTY>
    <TYPE>TTL</TYPE>
</SOURCE>    
<TARGET>
    <ID>target_source</ID>
    <ENDPOINT>/Users/gatemezing/Mondeca/projects/ProjetINSEE/limes/limes-core/agrovoc_lod.rdf</ENDPOINT>
    <VAR>?t</VAR>
    <PAGESIZE>5000</PAGESIZE>
    <RESTRICTION>?t rdf:type skos:Concept</RESTRICTION>
    <PROPERTY>skos:prefLabel AS lowercase RENAME agrovocname</PROPERTY>
    <PROPERTY>skos:altLabel AS lowercase RENAME agrovocaltname</PROPERTY>
	<TYPE>TTL</TYPE>
</TARGET>

<METRIC>
  AND(exactmatch(l.inseename,t.agrovocname)|0.9, exactmatch(l.inseealtname,t.agrovocaltname)|0.9)
</METRIC>
<ACCEPTANCE>
  <THRESHOLD>0.99</THRESHOLD>
  <FILE>results/accepted-combined-agrovoc.nt</FILE>
  <RELATION>skos:exactMatch</RELATION>
</ACCEPTANCE>
<REVIEW>
  <THRESHOLD>0.90</THRESHOLD>
  <FILE>results/reviewme-combined-agrovoc.nt</FILE>
  <RELATION>skos:exactMatch</RELATION>
</REVIEW>
<EXECUTION>
  <REWRITER>DEFAULT</REWRITER>
  <PLANNER>DEFAULT</PLANNER>
  <ENGINE>DEFAULT</ENGINE>
</EXECUTION>
<OUTPUT>NT</OUTPUT>
</LIMES>
